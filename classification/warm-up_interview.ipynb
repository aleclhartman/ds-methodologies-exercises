    "# gotcha python question\n",
    "# don't use objects as default function parameters\n",
    "def f(x, l=[]):\n",
    "    l.append(x)\n",
    "    return l\n",
    "\n",
    "l1 = f(1)\n",
    "l2 = f(2)\n",
    "l3 = f(3)\n",
    "\n",
    "# l2[-1] = \"?\"\n",
    "\n",
    "print(l1)\n",
    "print(l2)\n",
    "print(l3)"
   ]
